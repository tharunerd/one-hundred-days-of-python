# Sheety
SHEETY_PRICES_ENDPOINT=https://api.sheety.co/xxxx/your-project/prices
SHEETY_TOKEN=your_sheety_bearer_token_if_any
SHEETY_RESOURCE_NAME=prices

# Kiwi (Tequila)
TEQUILA_API_KEY=your_tequila_api_key
HOME_IATA=DEL         # your origin airport code (e.g., DEL, BLR, BOM)

# Search settings (optional tweaks)
CURRENCY=INR
MIN_NIGHTS=7
MAX_NIGHTS=28
SEARCH_MONTHS_AHEAD=6
MAX_STOPOVERS=0

# Twilio
TWILIO_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
FROM_NUMBER=+1xxxxxxxxxx
TO_NUMBER=+91xxxxxxxxxx
